.col-xs-8
  %h1 All orders

  %table.table
    %thead
      %th fasdfasd
      %th fasdfasd
      %th fasdfasd
      %th dfdafsfa
    %tbody
      %tr{ ng_repeat: "order in orders" }
        %td
          %a{ ng_click: "fetch_line_items(order)" }
            {{ order.id }}
        %td {{ order.description }}
        %td {{ order.status }}
        %td {{ order.confirmed_date  }}

  %ul
    %li{ ng_repeat: "line_item in line_items" }
      {{ line_item.value }}
